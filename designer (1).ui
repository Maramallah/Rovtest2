/*   GLOBAL APP BACKGROUND */
/* ======================= */
QWidget {
    background-color: #121212;   /* deep black */
    color: #e8e8e8;              /* white/gray text */
    font-size: 14px;
    font-family: Segon Ui, Arial;
}

/* ======================= */
/*       PUSH BUTTONS       */
/* ======================= */
QPushButton {
    background-color: #1f1f1f;
    color: #eaeaea;
    border: 1px solid #333;
    padding: 8px 14px;
    border-radius: 6px;
}

QPushButton:hover {
    background-color: #2d2d2d;
}

QPushButton:pressed {
    background-color: #3a3a3a;
    border-color: #555;
}

/* ======================= */
/*        LINE EDITS        */
/* ======================= */
QLineEdit, QTextEdit, QPlainTextEdit {
    background-color: #1a1a1a;
    color: white;
    border: 1px solid #333;
    border-radius: 5px;
    padding: 6px;
}

/* ======================= */
/*       COMBO BOXES        */
/* ======================= */
QComboBox {
    background-color: #1a1a1a;
    color: white;
    border: 1px solid #333;
    border-radius: 5px;
    padding: 6px;
}

QComboBox QAbstractItemView {
    background-color: #1a1a1a;
    selection-background-color: #333;
    color: white;
}

/* ======================= */
/*         SLIDERS          */
/* ======================= */
QSlider::groove:horizontal {
    height: 6px;
    background: #333;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #666;
    width: 14px;
    border-radius: 7px;

}
QSlider::groove:vertical {
    background: #333;          /* dark groove */
    width: 6px;                /* slim line */
    border-radius: 3px;
}

QSlider::handle:vertical {
    background: #666;          /* gray handle */
    border: 1px solid #444;
    height: 18px;
    width: 18px;
    margin: -4px 0;            /* allow overlap */
    border-radius: 9px;
}

QSlider::add-page:vertical {
    background: #222;           /* down side */
    border-radius: 3px;
}

QSlider::sub-page:vertical {
    background: #555;           /* up side */
    border-radius: 3px;
}
/* ======================= */
/*         TABLES           */
/* ======================= */
QTableWidget, QTableView {
    background-color: #151515;
    gridline-color: #444;
    border: 1px solid #333;
    color: white;
}

QHeaderView::section {
    background-color: #202020;
    color: #e8e8e8;
    padding: 6px;
    border: 0px;
    border-right: 1px solid #333;
}

QTableWidget::item:selected {
    background-color: #333;
    color: white;
}

/* ======================= */
/*          TABS            */
/* ======================= */
QTabWidget::pane {
    border: 1px solid #333;
    background: #111;
}

QTabBar::tab {
    background: #1d1d1d;
    color: #ddd;
    padding: 8px 20px;
    border-radius: 6px;
    margin-right: 4px;
}

QTabBar::tab:selected {
    background: #333333;
    color: white;
}

QTabBar::tab:hover {
    background: #2a2a2a;
}

/* ======================= */
/*       SCROLLBARS         */
/* ======================= */
QScrollBar:vertical {
    background: #1a1a1a;
    width: 12px;
}

QScrollBar::handle:vertical {
    background: #444;
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover {
    background: #555;
}



/* ======================= */
/*          FRAMES          */
/* ======================= */
QFrame {
    border: 1px solid #222;
    border-radius: 6px;
    background-color: #1a1a1a;
}
/======================/ 
/*      Prograssing Bar    */
/======================/ 
QProgressBar {
    border: 2px solid #444;
    border-radius: 5px;
    background-color: #222;
    text-align: center;
    color: white;
}

QProgressBar::chunk {
    background-color: #00b050;   /* اللون الأخضر */
    width: 10px;
}</string>
